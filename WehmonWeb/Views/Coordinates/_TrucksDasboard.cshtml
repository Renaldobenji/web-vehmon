@model WehmonWeb.Models.MapsStartupModel

<div style="position: fixed; margin-top: -40px" class="col-lg-2 col-md-offset-2">
    <div class="panel-group" id="accordionCVeh">
        <div class="panel panel-default">
            <div id="chatHeadingVeh" class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordionCVeh" href="#vcollapseOne">Vehicles</a>
            
                            <label style="font-weight: lighter;font-size: 16px">
                                <input id="emulate" type="checkbox">
                               Emulate Location
                            </label>
                   
                </h4>
            </div>
            <div id="vcollapseOne" class="panel-collapse collapse">
                <div style="height: 300px; max-height: 300px; overflow: scroll">
                    @foreach (var truck in Model.Trucks)
                    {
                  
                        <div style="margin-left: 7px" class="checkbox">
                            <label>
                                <input data-userId="@truck.UserId" class="userTruck" type="checkbox">
                                @truck.UserName
                            </label>
                        </div>
     
                    }

                </div>
            </div>
        </div>
    </div>
</div>
